[alias]
  unstage = reset HEAD --
  vdiff = !/home/deshwal/bin/git_vimdiff.pl
  vdiffp = !git vdiff "$1"~
  diffp = !git diff "$1"~
  graph = log --graph --pretty=oneline --decorate --abbrev-commit
[color]
  ui = true
  branch = true
  diff = true
  interactive = true
[treediff]
  tool = vimdiff.py
[format]
  pretty = short
[user]
  name = deshwal
  email = deshwal@thoughtspot.com
[core]
  preloadindex = true
